{
  "extends": "airbnb-base",
  "env": {
    "es2021": true
  },
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "no-console": 0,
    "import/prefer-default-export": 0,
    // "semi": 0,
    "quotes": [2, "double",  "single", { "avoidEscape": true, "allowTemplateLiterals": true }]

  }
}
